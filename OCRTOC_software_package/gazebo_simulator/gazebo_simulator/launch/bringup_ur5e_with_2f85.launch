<?xml version="1.0"?>
<launch>
  <arg name="scene" default="1-1" doc="Scene id." />
  <arg name="limited" default="true"  doc="If true, limits joint range [-PI, PI] on all joints." />
  <arg name="paused" default="false" doc="Starts gazebo in paused mode" />
  <arg name="gui" default="true" doc="Starts gazebo gui" />
  <arg name="gripper" default="robotiq_2f_85"  doc="Attach gripper on manipulator" />
  <arg name="materials_dir" default="/home/siddharth/backup_18.04/ocrtoc_materials"  doc="The models and scenes path" />

  <env name="GAZEBO_MODEL_PATH" value="$(arg materials_dir)/models" />

  <!-- startup simulated world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(arg materials_dir)/scenes/$(arg scene)/target.world"/>
    <!-- value is true to set up the robot joints -->
    <arg name="paused" value="false"/>
    <arg name="gui" value="$(arg gui)"/>
  </include>

  <!-- Spawning launch file to add a ground model-->
  <include file ="$(find gazebo_simulator)/launch/spawn_ground.launch">
    <arg name="robot_name" value="ground" />
    <arg name="sdf_robot_file" value="$(find gazebo_simulator)/models/ground/model.sdf" />
  </include>

     <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find motoman_motomini_support)/urdf/motomini.xacro'" />
 	

    <node name="mybot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" 
	    args="-urdf -param robot_description -model motoman_motomini" respawn="false"/>

  <!-- load robot definition -->
  <!--include file="$(find gazebo_simulator)/launch/ur5e_upload.launch">
    <arg name="limited" value="$(arg limited)"/>
    <arg name="gripper" value="$(arg gripper)" />
  </include-->

 
  <!--group if="$(arg paused)">
    <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model"
          args="-urdf -param robot_description -model robot -J shoulder_pan_joint -1.5707 -J shoulder_lift_joint -1.5707 -J elbow_joint 1.5707 -J wrist_1_joint 0 -J wrist_2_joint 0"
          respawn="false" output="screen" />
  </group>

  
  <group unless="$(arg paused)">
  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model"
        args="-urdf -param robot_description -model robot -J shoulder_pan_joint -1.5707 -J shoulder_lift_joint -1.5707 -J elbow_joint 0 -J wrist_1_joint 0 -J wrist_2_joint 0 -unpause"
        respawn="false" output="screen" />
  </group-->

  
  <!--include file="$(find ocrtoc_solution)/launch/controllers.launch"/-->
  <include file="$(find motoman_motomini_support)/launch/controllers.launch"/>
  <!--include file="$(find motoman_motomini_support)/launch/moveit_ur5.launch"/-->
  <!--include file="$(find haf_grasping)/launch/haf_grasping_all.launch"/-->
</launch>
