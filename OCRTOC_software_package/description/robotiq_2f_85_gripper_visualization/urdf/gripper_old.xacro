<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="gripper">

  <xacro:macro name="gripper_mpo" params="prefix parent *origin">
  <link name= "${prefix}link5">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
         <mesh filename="package://robotiq_2f_85_gripper_visualization/meshes/visual/link5.stl" scale="0.3 0.3 0.3"/>    
        </geometry>
        <material name = "light_grey">
         <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
    </visual>
    
    <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
         <mesh filename="package://robotiq_2f_85_gripper_visualization/meshes/visual/link5.stl" scale="0.3 0.3 0.3"/>    
        </geometry>    
    </collision>

    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <mass value= "0.7"/>
        <inertia ixx= "0.0003529" ixy="-0" ixz="0" iyy="0.0007598" iyz="-0" izz="0.0007598" /> 
    </inertial>
</link>



<link name= "${prefix}finger1">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
        <mesh filename="package://robotiq_2f_85_gripper_visualization/meshes/visual/fin1.stl" scale="0.3 0.3 0.3"/>    
        </geometry>
        <material name = "light_grey">
         <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
    </visual>
    
    <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
         <mesh filename="package://robotiq_2f_85_gripper_visualization/meshes/visual/fin1.stl" scale="0.3 0.3 0.3"/>    
        </geometry>    
    </collision>

    <inertial>
        <origin xyz="-0.015 0.07 0.0" rpy="0 0 0.15" />
        <mass value= "0.7"/>
        <inertia ixx= "0.001607" ixy="0" ixz="-0" iyy="0.00005536" iyz="-0" izz="0.0016246" /> 
    </inertial>
</link>

<link name= "${prefix}finger2">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
         <mesh filename="package://robotiq_2f_85_gripper_visualization/meshes/visual/fin2.stl" scale="0.3 0.3 0.3"/>    
        </geometry>
        <material name = "light_grey">
         <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
    </visual>
    
    <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
         <mesh filename="package://robotiq_2f_85_gripper_visualization/meshes/visual/fin2.stl" scale="0.3 0.3 0.3"/>    
        </geometry>    
    </collision>

    <inertial>
         <origin xyz="0.015 0.07 0.0" rpy="0 0 -0.15" />
        <mass value= "0.7"/>
        <inertia ixx= "0.001607" ixy="0" ixz="-0" iyy="0.00005536" iyz="-0" izz="0.0016246" />  
    </inertial>
</link>

<joint name="${prefix}link5_joint" type="fixed">
    <parent link="${parent}"/>
    <child link="${prefix}link5"/>
    <xacro:insert_block name="origin" />
    <!--<limit lower= "-3.0" upper= "3.0" effort="200" velocity="3" />
    <axis xyz = "0 0 1" />-->
</joint>


<link name="gripper_ee_link"/>
<joint name="ee_joint" type="fixed">
    <origin xyz="0 0 0.107" rpy="-1.5707 -1.5707 0"/>
	<parent link="${prefix}link5"/>
	<child link="gripper_ee_link"/>
</joint>


<joint name="${prefix}l_finger_joint" type="prismatic">
    <origin rpy="1.5708 0 0" xyz="0.024 0 0.0227" />
    <parent link="${prefix}link5" />
    <child link="${prefix}finger1" />
    <axis xyz="1 0 0" />
  <limit effort="60" lower="0" upper="0.04" velocity="0.07" /><dynamics damping="100.0" />
</joint>

<joint name="${prefix}r_finger_joint" type="prismatic">
    <origin rpy="1.5708 0 0" xyz="-0.024 0 0.0227" />
    <parent link="${prefix}link5" />
    <child link="${prefix}finger2" />
    <axis xyz="-1 0 0" />
    <limit effort="60" lower="0" upper="0.04" velocity="0.07" /><dynamics damping="100.0" />
</joint>

<gazebo reference="${prefix}link5">
    <mu1>5.0</mu1>
    <mu2>5.0</mu2>
    <material>Gazebo/Grey</material>
    <turnGravityOff>false</turnGravityOff>
</gazebo>

<gazebo reference="${prefix}finger1">
    <implicitSpringDamper>1</implicitSpringDamper>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
    <mu1>10000.0</mu1>
    <mu2>10000.0</mu2>
    <minDepth>0.001</minDepth>
    <material>Gazebo/Grey</material>
    <turnGravityOff>false</turnGravityOff>
</gazebo>

<gazebo reference="${prefix}finger2">
    <implicitSpringDamper>1</implicitSpringDamper>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
    <mu1>100000</mu1>
    <mu2>100000</mu2>
    <minDepth>0.001</minDepth>
    <material>Gazebo/Grey</material>
    <turnGravityOff>false</turnGravityOff>
</gazebo>

<!--<transmission name="${prefix}link5_control">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="${prefix}link5_joint">
     <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="${prefix}link5_motor">
     <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
     <mechanicalReduction>2</mechanicalReduction>
    </actuator>
</transmission>-->

<transmission name="${prefix}link5_finger1_control">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="${prefix}l_finger_joint">
     <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="${prefix}l_finger_motor">
     <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
     <mechanicalReduction>2</mechanicalReduction>
    </actuator>
</transmission>

<!--transmission name="${prefix}link5_finger2_control">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="${prefix}r_finger_joint">
     <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="${prefix}r_finger_motor">
     <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
     <mechanicalReduction>2</mechanicalReduction>
    </actuator>
</transmission--> 

<gazebo>
    <plugin name="${prefix}link5_finger2_control" filename="libroboticsgroup_gazebo_mimic_joint_plugin.so">
    <joint>${prefix}l_finger_joint</joint>
    <mimicJoint>${prefix}r_finger_joint</mimicJoint>
    <multiplier>1</multiplier>
    </plugin>
</gazebo>

  </xacro:macro>
  
</robot>


