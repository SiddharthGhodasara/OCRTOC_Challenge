<joint name="world_joint" type="fixed">
    <parent link="${parent}"/>
    <child link="table"/>
    <xacro:insert_block name="origin" />
    <!--<limit lower= "-3.0" upper= "3.0" effort="200" velocity="3" />
    <axis xyz = "0 0 1" />-->
</joint>
  <joint name="leg1_joint" type="fixed">
    <parent link="table"/>
    <child link="leg1"/>
    <origin rpy="0.0 0.0 0" xyz="0.4 0.4 -0.35"/>
  </joint>
  <joint name="leg2_joint" type="fixed">
    <parent link="table" />
    <child link = "leg2" />
    <origin xyz="0.4 -0.4 -0.35" rpy="0 0 0" />
  </joint>
  <joint name="leg3_joint" type="fixed">
    <parent link="table"/>
    <child link="leg3"/>
    <origin xyz="-0.4 0.4 -0.35" rpy="0 0 0" />
  </joint>
