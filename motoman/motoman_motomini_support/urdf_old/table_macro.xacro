<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_motomini_table" params="prefix parent *origin">

<link name="table">
  <origin xyz="0.0 0.0 0" rpy="0 0 0" />
  <inertial>
          <mass value="5" />
          <origin xyz="0 0 0" />
          <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1" />
    </inertial>
    <visual>
      <geometry>
        <box size="0.83 0.830 0.001"/>
      </geometry>
        <material name = "light_grey">
         <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
    </visual>
    <collision>
      <geometry>
         <box size="0.83 0.830 0.001"/>
      </geometry>
    </collision>
  </link>

