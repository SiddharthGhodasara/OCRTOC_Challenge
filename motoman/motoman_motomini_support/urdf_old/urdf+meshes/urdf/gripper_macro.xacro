<joint name="gripper_joint" type="fixed">
    <parent link="${parent}"/>
    <child link="gripper_base"/>
    <xacro:insert_block name="origin" />
    <!--<limit lower= "-3.0" upper= "3.0" effort="200" velocity="3" />
    <axis xyz = "0 0 1" />-->
</joint>
<joint name="claw_1b_joint" type="revolute">
    <parent link="gripper_base"/>
    <child link="claw_1b"/>
    <limit lower= "-3.0" upper= "3.0" effort="200" velocity="3" />
    <axis xyz = "0 0 1" />
    <origin rpy="0.0 0.0 0" xyz="0.045 0.01154 0.017503"/>
  </joint>
<joint name="claw_1a_joint" type="revolute">
    <parent link="gripper_base"/>
    <child link="claw_1a"/>
    <limit lower= "-3.0" upper= "3.0" effort="200" velocity="3" />
    <axis xyz = "0 0 1" />
    <origin rpy="0.0 0.0 0" xyz="0.045 -0.01154 0.017503"/>
    <mimic joint="claw_1b_joint" multiplier="-1"/>
  </joint>

<joint name="claw_2a_joint" type="revolute">
